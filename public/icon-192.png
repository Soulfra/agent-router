<?xml version="1.0" encoding="UTF-8"?>
<svg width="192" height="192" viewBox="0 0 192 192" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f3460;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#00ff88;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="192" height="192" fill="url(#bgGradient)" rx="28.799999999999997"/>

  <!-- Main icon shape - Abstract "C" representing CalOS -->
  <g transform="translate(96, 96)">
    <!-- Outer ring -->
    <circle cx="0" cy="0" r="57.599999999999994" fill="none"
            stroke="url(#accentGradient)" stroke-width="9.600000000000001"
            stroke-dasharray="90.24 90.24"
            stroke-linecap="round"
            transform="rotate(-45)"/>

    <!-- Inner glow -->
    <circle cx="0" cy="0" r="28.799999999999997" fill="#00d4ff" opacity="0.3"/>

    <!-- Center dot -->
    <circle cx="0" cy="0" r="15.36" fill="#00ff88"/>
  </g>

  <!-- Text label (only on larger icon) -->
  
</svg>